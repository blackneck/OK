{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "modules": "./src/modules",
          "app": "./src",
          "common": "./src/common"
        }
      }
    ]
  ]
}
